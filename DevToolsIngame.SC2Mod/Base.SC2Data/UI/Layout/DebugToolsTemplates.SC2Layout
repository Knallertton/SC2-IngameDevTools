		<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
		<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
		<Color val="75,75,75"/>
		<RenderPriority val="509"/>
		<Alpha val="255"/>
		<Tiled val="true"/>
	</Frame>

	<Frame type="Button" name="HotkeyGroupButtonTemplate">
		<!-- <HitTestFrame val="HitTestFrame"/> -->
		<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
		<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
		<NormalImage val="NormalImage"/>
		<HoverImage val="HoverImage"/>
		<Style val="@@HotkeyGroupButtonStyle"/>
		<Height val="28"/>
		<Visible val="false"/>
		<CollapseLayout val="true"/>
		<AcceptsMouse val="{$ancestor[@name=DebugTabControl]/@AcceptsMouse}"/>

		<Frame type="Image" name="NormalImage" template="DebugToolsTemplates/HotkeyGroupButtonNormal">
		</Frame>
		<Frame type="Image" name="HoverImage" template="DebugToolsTemplates/HotkeyGroupButtonHover">
		</Frame>
		<Frame type="Label" name="Label" template="StandardGreyboxTemplates/GenericButtonLabelTemplate">
			<Anchor side="Left" relative="$parent/ExpandImage" pos="Max" offset="5"/>
			<Anchor side="Top" relative="$parent" pos="Min" offset="2"/>
			<Anchor side="Bottom" relative="$parent" pos="Max" offset="2"/>
		</Frame>

		<!-- <StateGroup name="Selected">
			<DefaultState val="False"/>
		
			<State name="False">
				<When type="Property" frame="$this" Toggled="False" />
				<Action type="SetProperty" frame="$this" NormalImage="NormalImage"/>
			</State>
		
			<State name="True">
				<When type="Property" frame="$this" Toggled="True" />
				<Action type="SetProperty" frame="$this" NormalImage="HoverImage"/>
			</State>
		</StateGroup> -->


		<!-- <StateGroup name="Expanded">
			<DefaultState val="False"/>
		
			<State name="False">
				<When type="Property" frame="$this" Toggled="False" />
				<Action type="SetProperty" frame="ExpandImage" StateIndex="0"/>
			</State>
		
			<State name="True">
				<When type="Property" frame="$this" Toggled="True" />
				<Action type="SetProperty" frame="ExpandImage" StateIndex="1"/>
			</State>
		</StateGroup> -->

		<Frame type="Image" name="ExpandImage">
			<Width val="22"/>
			<Height val="22"/>
			<Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
			<Anchor side="Left" relative="$parent" pos="Min" offset="6"/>
			<Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>

			<!-- UI/HotkeyDialogTreeButtonNormal=Assets\Textures\ui_button_expandcollapselist_normalpressed.dds
			UI/HotkeyDialogTreeButtonHover=Assets\Textures\ui_button_expandcollapselist_normaloverpressedover.dds -->

			<LayerCount val="2"/>
			<TextureCoords top="0.0" left="1.0" bottom="1.0" right="0.0" layer="0"/>
			<TextureCoords top="1.0" left="1.0" bottom="0.0" right="0.0" layer="1"/>
			<Texture val="@@UI/HotkeyDialogTreeButtonNormal" layer="0"/>
			<Texture val="@@UI/HotkeyDialogTreeButtonNormal" layer="1"/>
			<LayerVisible val="true" layer="0"/>
			<LayerVisible val="false" layer="1"/>
			<TextureType val="Normal" layer="0"/>
			<TextureType val="Normal" layer="1"/>
			<LayerColor val="##StandardBorderHotkey" layer="0"/>
			<LayerColor val="##StandardBorderHotkey" layer="1"/>
			<StateCount val="2" layer="0"/>
			<StateCount val="2" layer="1"/>

			<StateGroup name="Hovering">
				<DefaultState val="False"/>
			
				<State name="False">
					<When type="Property" frame="$parent" Hovering="False" />
					<Action type="SetProperty" frame="$this" Texture="@@UI/HotkeyDialogTreeButtonNormal" index="0"/>
					<Action type="SetProperty" frame="$this" Texture="@@UI/HotkeyDialogTreeButtonNormal" index="1"/>
				</State>

				<State name="True">
					<When type="Property" frame="$parent" Hovering="True"/>
					<Action type="SetProperty" frame="$this" Texture="@@UI/HotkeyDialogTreeButtonHover" index="0"/>
					<Action type="SetProperty" frame="$this" Texture="@@UI/HotkeyDialogTreeButtonHover" index="1"/>
				</State>
			</StateGroup>

			<StateGroup name="Toggled">
				<DefaultState val="False"/>
			
				<State name="False">
					<When type="Property" frame="$parent" Toggled="False" />
					<Action type="SetProperty" frame="$this" LayerVisible="True" index="0"/>
					<Action type="SetProperty" frame="$this" LayerVisible="False" index="1"/>
				</State>

				<State name="True">
					<When type="Property" frame="$parent" Toggled="True"/>
					<Action type="SetProperty" frame="$this" LayerVisible="False" index="0"/>
					<Action type="SetProperty" frame="$this" LayerVisible="True" index="1"/>
				</State>
			</StateGroup>

			<StateGroup name="Pushed">
				<DefaultState val="False"/>
			
				<State name="False">
					<When type="Property" frame="$parent" Pushed="False" />
					<Action type="SetProperty" frame="$this" StateIndex="0" index="0"/>
					<Action type="SetProperty" frame="$this" StateIndex="1" index="1"/>
				</State>
				
				<State name="True">
					<When type="Property" frame="$parent" Pushed="True" />
					<Action type="SetProperty" frame="$this" StateIndex="1" index="0"/>
					<Action type="SetProperty" frame="$this" StateIndex="0" index="1"/>
				</State>

			</StateGroup>

		</Frame>

		<Toggleable val="true"/>
		<Toggled val="False"/>
	</Frame>

	<Frame type="Frame" name="HotkeyGroupFrameTemplate">
		<Anchor side="Bottom" relative="List" pos="Max" offset="0"/>
		<CollapseLayout val="true"/>
		<Visible val="{List/@Visible}"/>
		<Frame type="Frame" name="List" template="DebugToolsTemplates/HotkeyListTemplate">
