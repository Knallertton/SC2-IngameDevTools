	<Constant name="SceneUIAnimationTime" val="1.0"/>

	<Frame type="Label" name="SceneUILabel">
		<!-- <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
		<Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
		<Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
		<Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/> -->

		<Width val="800"/>
		<Height val="100"/>
		<RenderType val="HDR"/>
		<Unclipped val="true"/>
		<BlendMode val="Add"/>
		<Blur val="true"/>
		<BlurAmount val="0"/>
		<WriteOutText val="true"/>
		<WriteOutTextDuration val="1000"/>
		<UseAlternateTime val="true"/>
		<ScaleText val="true"/>
		<Animation name="Animation" speed="2.0">
			<Event event="Create" action="DirectionForward,Play" frame="$this"/>
			<Event event="Destroy" action="DirectionReverse,Play" frame="$this"/>
			<Controller type="Fade" end="Pause" frame="$this">
				<Key type="Curve" time="0.0" value="0"/>
				<Key type="Curve" time="#SceneUIAnimationTime" value="255"/>
			</Controller>
			<Controller type="Blur" end="Pause" frame="$this">
				<Key type="Curve" time="0.0" value="2"/>
				<Key type="Curve" time="#SceneUIAnimationTime" value="0"/>
			</Controller>
		</Animation>
	</Frame>

	<Frame type="EditBox" name="ListenerBoxTemplate">
		<Alpha val="0"/>
		<AcceptsMouse val="false"/>
		<RejectsFocus val="true"/>
		<Style val="DebugTiny"/>
		<Frame type="Label" name="Text">
			<Visible val="false"/>
			<Style val="DebugTiny"/>
			<Alpha val="0"/>
		</Frame>
	</Frame>

