			</State>
			<State name="False"/>
		</StateGroup> -->

		<StateGroup name="IsActiveSelection">
			<DefaultState val="False"/>
			<State name="True">
				<When type="Property" frame="SelectedIndex" operator="NotEqual" Text="-2"/>
				<Action type="SetProperty" frame="DummyFrame" Visible="False" />
				<Action type="SendEvent" frame="$parent/$parent" event="Deselect"/>
				<Action type="SetProperty" frame="DummyFrame" Visible="True" />
			</State>
			<State name="False"/>
		</StateGroup>

		<!-- <Animation name="DeselectForward">
			<Event event="Deselect" action="Reset,Play" frame="$this"/>
			<Event event="OnHidden" action="Reset,Play" frame="$this"/>
			<Controller type="Event" end="Stop" frame="DummyFrame">
				<Key type="Event" time="0.0" event="Deselect"/>
			</Controller>
		</Animation> -->

		<Frame type="Frame" name="DummyFrame">
			<Anchor relative="$parent"/>
			<Alpha val="0"/>
			<Visible val="True"/>
			<Animation name="Deselect" flags="NoEventsWhileHidden">
				<!-- <Event event="Deselect" action="Reset,Play" frame="$this"/> -->
				<Event event="Deselect" action="Reset,Play" frame="$parent/$parent/$parent"/>
				<Controller type="Property" end="Pause" frame="$parent/SelectedIndex" property="Text">
					<Key type="Property" time="0.0" value="-2"/>
				</Controller>
			</Animation>	
		</Frame>

		<Frame type="CountdownLabel" name="IndexCounter">
			<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
			<UseAlternateTime val="True"/>
			<CountdownTime val="0"/>
			<StartValue val="{$this/@CurrentValue}"/>
			<TargetValue val="{Math/@ResultInt}"/>
			<Style val="DebugTiny"/>
			<CurrentValue val="0"/>
			<Text val="0"/>
			<Alpha val="0"/>

			<Animation name="Counter">
				<Event event="ItemAdded" action="Reset,Play" frame="$parent"/>
				<Controller type="Counter" end="Pause" frame="$this">
					<Key type="Curve" time="0.0" value="1.0"/>
				</Controller>
				<Controller type="Event" end="Stop" frame="$this">
					<Key type="Event" time="0.0" event="CountdownLabelResetRequest"/>
				</Controller>
			</Animation>

			<Frame type="MathFrame" name="Math">
				<A val="{$parent/@StartValue}"/>
				<B val="1"/>
				<Operator val="Add"/>
			</Frame>
		</Frame>

		<Frame type="EditBox" name="DataCount" template="DebugToolsTemplates/DummyBoxTemplate">
			<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Left" relative="$parent" pos="Max" offset="0"/>
			<Text val="{$parent/IndexCounter/@CurrentValue}"/>
		</Frame>
		
