		<StateGroup name="AnimToggle">
			<DefaultState val="NotActive"/>
		
			<State name="NotActive">
				<When type="Property" frame="$this" operator="Equal" Enabled="True"/>
				<Action type="SetAnimationProperty" frame="$this" animation="MouseOver" enabled="False"/>
				<Action type="SendEvent" frame="$this" event="FadeIn"/>
			</State>
		
			<State name="Active">
				<When type="Property" frame="$this" operator="Equal" Enabled="False"/>
				<Action type="SetAnimationProperty" frame="$this" animation="MouseOver" enabled="True"/>
				<Action type="SendEvent" frame="$this" event="FadeOut"/>
			</State>
		</StateGroup>
		<Animation name="Fade">
			<Event event="FadeOut" action="DirectionReverse,Play" frame="$this"/>
			<Event event="FadeIn" action="DirectionForward,Play" frame="$this"/>
			<Controller type="Fade" end="Pause" frame="$this">
				<Key type="Curve" time="0.0" value="0"/>
				<Key type="Curve" time="0.125" value="255"/>
			</Controller>
		</Animation>
		<Animation name="MouseOver">
			<Event event="OnMouseEnter" action="DirectionForward,Play" frame="$parent/TimelineHitbox"/>
			<Event event="OnMouseEnter" action="DirectionForward,Play" frame="$parent/TimelineSlider"/>
			<Event event="OnMouseExit" action="DirectionReverse,Play" frame="$parent/TimelineHitbox"/>
			<Event event="OnMouseExit" action="DirectionReverse,Play" frame="$parent/TimelineSlider"/>
			<Controller type="Fade" end="Pause" frame="$this">
				<Key type="Curve" time="0.0" value="0"/>
				<Key type="Curve" time="0.125" value="255"/>
			</Controller>
		</Animation>
	</Frame>

